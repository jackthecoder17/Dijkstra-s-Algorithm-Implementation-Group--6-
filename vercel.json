{
    "version": 2,
    "builds": [{
        "src": "wsgi.py",
        "use": "@vercel/python"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "wsgi.py"
    }],
    "env": {
        "SECRET_KEY": "django-insecure-city-distance-calculator-production-key-2024",
        "DEBUG": "False",
        "ALLOWED_HOSTS": "*",
        "CORS_ALLOW_ALL_ORIGINS": "True"
    }
}